{
    "manifest_version": 3,
    "name": "Hermes",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting",
      "webNavigation"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "*://*/"
    ],
    "action": {
      "default_icon": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
      }
    }
  }
  